{
    "builds": [{
        "src": "tugas_kelompok_basdat/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { 
            "maxLambdaSize": "15mb",
            "runtime": "python3.10"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "tugas_kelompok_basdat/wsgi.py"
        }
    ]
}